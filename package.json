{
	"name": "tabnine-plus",
	"displayName": "TabNine+",
	"description": "TabNine client (all-language autocompleter) for the VS Code.",
	"version": "1.0.0",
	"publisher": "svipas",
	"icon": "icon.png",
	"main": "./dist/main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/svipas/vscode-tabnine-plus.git"
	},
	"bugs": {
		"url": "https://github.com/svipas/vscode-tabnine-plus/issues"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"extensionKind": [
		"workspace"
	],
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "TabNine::config",
				"title": "TabNine: open config"
			},
			{
				"command": "TabNine::restart",
				"title": "TabNine: restart"
			},
			{
				"command": "TabNine::sem",
				"title": "TabNine: enable semantic completion for current language"
			},
			{
				"command": "TabNine::no_sem",
				"title": "TabNine: disable semantic completion for current language"
			}
		],
		"configuration": [
			{
				"type": "object",
				"title": "TabNine",
				"properties": {
					"tabnine.debug": {
						"type": "boolean",
						"default": false,
						"description": "Enables debug mode.",
						"scope": "window"
					},
					"tabnine.maxNumberOfResults": {
						"type": "number",
						"default": 5,
						"minimum": 1,
						"maximum": 5,
						"description": "Maximum number of results returned by TabNine.",
						"scope": "window"
					},
					"tabnine.disabledLanguagesIds": {
						"type": "array",
						"items": {
							"type": "string"
						},
						"description": "List of languages IDs to disable.",
						"scope": "window"
					}
				}
			}
		]
	},
	"scripts": {
		"watch": "tsc -watch -p ./",
		"compile": "tsc -p ./",
		"vscode:publish": "vsce publish --yarn",
		"vscode:prepublish": "yarn compile",
		"vscode:uninstall": "node ./dist/uninstall.js"
	},
	"devDependencies": {
		"@types/node": "^14.0.5",
		"@types/vscode": "^1.30.0",
		"typescript": "^3.9.3"
	},
	"keywords": [
		"tabnine",
		"codota",
		"client",
		"code",
		"autocomplete",
		"autocompleter",
		"suggestions",
		"suggest",
		"ai",
		"ml",
		"artificial intelligence",
		"machine learning",
		"deep learning",
		"help",
		"faster",
		"discover",
		"typing",
		"type"
	]
}

{
	"name": "tabnine-plus",
	"displayName": "TabNine+",
	"description": "TabNine client (all-language autocompleter) for the VS Code.",
	"version": "1.0.0",
	"publisher": "svipas",
	"icon": "icon.png",
	"main": "./dist/main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/svipas/vscode-tabnine-plus.git"
	},
	"bugs": {
		"url": "https://github.com/svipas/vscode-tabnine-plus/issues"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"extensionKind": [
		"workspace"
	],
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "TabNine::config",
				"title": "TabNine: config"
			}
		],
		"configuration": [
			{
				"title": "TabNine",
				"properties": {
					"tabnine.disable_line_regex": {
						"type": "array",
						"default": [],
						"description": "Disable TabNine when the current line contains a match of any of the provided regexes. For example, add \"require\" to disable TabNine when the current line contains the word 'require'."
					},
					"tabnine.disable_file_regex": {
						"type": "array",
						"default": [],
						"description": "Disable TabNine when the file path contains a match of any of the provided regexes. For example, add \"[.]js$\" to disable TabNine in JavaScript files."
					}
				}
			}
		]
	},
	"scripts": {
		"watch": "tsc -watch -p ./",
		"compile": "tsc -p ./",
		"vscode:publish": "vsce publish --yarn",
		"vscode:prepublish": "yarn compile",
		"vscode:uninstall": "node ./dist/uninstall.js"
	},
	"dependencies": {
		"semver": "^5.1.1",
		"await-semaphore": "^0.1.3"
	},
	"devDependencies": {
		"@types/node": "^6.0.40",
		"typescript": "^2.1.4",
		"vscode": "^1.1.21"
	},
	"keywords": [
		"tabnine",
		"codota",
		"client",
		"code",
		"autocomplete",
		"autocompleter",
		"suggestions",
		"suggest",
		"ai",
		"ml",
		"artificial intelligence",
		"machine learning",
		"deep learning",
		"help",
		"faster",
		"discover",
		"typing",
		"type"
	]
}
